{
  "name": "QA Admin",
  "version": "1.0.4",
  "permissions": [
     "cookies", "tabs", "http://*/*", "https://*/*", "contextMenus", "storage", "notifications"
  ],
  "browser_action": {
      "default_title": "Writer",
      "default_icon": "images/writer-logo.png"
  },
    "content_security_policy": "script-src 'self' https://smtpjs.com/; object-src 'self'",

  "background": {
    "scripts": ["js/jquery-2.0.2.min.js","js/jquery.ajaxupload.js","js/background.js"]
  },
  "icons":{
         "128":"images/writer-logo.png"
     },
  "manifest_version": 2,
  "incognito":"split",
  "update_url": "http://clients2.google.com/service/update2/crx"

  }
